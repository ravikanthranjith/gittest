                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @InStr VARCHAR(MAX)
SET @InStr= ?
declare @temptab TABLE
(tempid int IDENTITY(1,1) not null, 
   
   Data NVARCHAR(MAX))
BEGIN
    ;-- Ensure input ends with comma
       SET @InStr = REPLACE(@InStr + ',', ',,', ',')
       DECLARE @SP INT
DECLARE @VALUE VARCHAR(1000)
WHILE PATINDEX('%,%', @INSTR ) &lt;&gt; 0 
BEGIN
   SELECT  @SP = PATINDEX('%,%',@INSTR)
   SELECT  @VALUE = LEFT(@INSTR , @SP - 1)
   SELECT  @INSTR = STUFF(@INSTR, 1, @SP, '')
   INSERT INTO @TempTab( Data) VALUES (@VALUE)
END
       
