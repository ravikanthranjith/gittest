                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{70CBB221-62D4-4E9F-9044-26658A02DCDC}"
                      SQLTask:SqlStatementSource="DECLARE @InStr VARCHAR(MAX)&#xA;SET @InStr= ?&#xA;declare @temptab TABLE&#xA;(tempid int IDENTITY(1,1) not null, &#xA;   &#xA;   Data NVARCHAR(MAX))&#xA;BEGIN&#xA;    ;-- Ensure input ends with comma&#xA;       SET @InStr = REPLACE(@InStr + ',', ',,', ',')&#xA;       DECLARE @SP INT&#xA;DECLARE @VALUE VARCHAR(1000)&#xA;WHILE PATINDEX('%,%', @INSTR ) &lt;&gt; 0 &#xA;BEGIN&#xA;   SELECT  @SP = PATINDEX('%,%',@INSTR)&#xA;   SELECT  @VALUE = LEFT(@INSTR , @SP - 1)&#xA;   SELECT  @INSTR = STUFF(@INSTR, 1, @SP, '')&#xA;   INSERT INTO @TempTab( Data) VALUES (@VALUE)&#xA;END&#xA;       &#xA;END&#xA;&#xA;&#xA;DELETE IB&#xA;FROM [dbo].[HFM_OUT_IS] AS IB&#xA;INNER JOIN dbo.HFM_PROCESS_STATUS_REPORT PS&#xA;ON PS.HFM_PROCESS_STATUS_REPORT_ID = IB.HFM_PROCESS_STATUS_REPORT_ID&#xA;WHERE IB.[ENTITY_LABEL] IN (SELECT Data FROM @TempTab)&#xA; AND [PERIOD_LABEL] = ?&#xA; AND PS.DATA_VERSION_ID = ?&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                      <SQLTask:ParameterBinding
