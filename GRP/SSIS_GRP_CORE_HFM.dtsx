                <DTS:Executable
                  DTS:refId="Package\SC\Looping through every request\SC_HFM_OUT_IS\EST_Exception"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Execute SQL Task"
                  DTS:Disabled="True"
                  DTS:DTSID="{aa69df43-bfac-4f11-95c9-1b781d660964}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="EST_Exception"
                  DTS:ThreadHint="3">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{70CBB221-62D4-4E9F-9044-26658A02DCDC}"
                      SQLTask:SqlStatementSource="DECLARE @HFM_PROCESS_STATUS_REPORT_ID UNIQUEIDENTIFIER&#xA;SET @HFM_PROCESS_STATUS_REPORT_ID  = ?&#xA;&#xA;IF OBJECT_ID('tempdb..#TMP_COUNT') IS NOT NULL &#xA;  DROP TABLE #TMP_COUNT; &#xA;&#xA;&#xA;SELECT &#xA; A.LEGAL_ENTITY&#xA;,B.ENTITY_LABEL&#xA;,A.HFM_PROCESS_PARAMETER_TYPE_DESC&#xA;,A.PARAMETER_VALUE&#xA;&#xA;INTO #TMP_COUNT&#xA;&#xA;&#xA;&#x9;FROM &#xA;&#x9;(&#xA;&#x9;&#x9;&#x9;&#x9;SELECT &#xA;&#x9;&#x9;&#x9;&#x9;  LE.LEGAL_ENTITY&#xA;&#x9;&#x9;&#x9;&#x9; ,HFM_PROCESS_PARAMETER_TYPE_DESC&#xA;&#x9;&#x9;&#x9;&#x9; ,HEP.PARAMETER_VALUE&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;FROM DBO.HFM_Execution_parameter HEP&#xA;&#x9;&#x9;&#x9;&#x9;JOIN dbo.HFM_PROCESS_STATUS_REPORT PS ON HEP.HFM_PROCESS_STATUS_REPORT_ID=ps.HFM_PROCESS_STATUS_REPORT_ID&#xA;&#x9;&#x9;&#x9;&#x9;JOIN [dbo].[HFM_PROCESS_PARAMETER_TYPE_REF] HParamTypeRef&#x9;on HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_ID=HEP.HFM_PROCESS_PARAMETER_TYPE_ID&#xA;&#x9;&#x9;&#x9;&#x9;JOIN [dbo].[LEGAL_ENTITY_HIERARCHY] LE &#xA;&#x9;&#x9;&#x9;&#x9;ON  HEP.PARAMETER_VALUE= CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_DESC='Legal Entity' THEN LE.LEGAL_ENTITY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_DESC='MFRP Unit' THEN LE.MFRP_UNIT&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE LE.MAN_COUNTRY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;JOIN DBO.PERIOD P ON  PS.PERIOD_ID=P.PERIOD_ID&#xA;&#x9;&#x9;&#x9;&#x9;JOIN DBO.data_version DV ON DV.DATA_VERSION_ID=PS.DATA_VERSION_ID&#xA;&#x9;&#x9;&#x9;&#x9;JOIN DBO.DATA_VERSION_TYPE DVT ON DV.DATA_VERSION_TYPE_ID=DVT.DATA_VERSION_TYPE_ID&#xA;&#x9;&#x9;&#x9;&#x9;WHERE HEP.HFM_PROCESS_STATUS_REPORT_ID = @HFM_PROCESS_STATUS_REPORT_ID&#xA;&#x9;&#x9;&#x9;&#x9;AND LE.VERSION_ID=1&#xA;&#x9;&#x9;&#x9;&#x9;AND DVT.DATA_VERSION_TYPE_DESC = 'Actuals'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) A&#xA;&#x9;LEFT OUTER JOIN &#xA;&#x9;(&#xA;&#x9;SELECT DISTINCT ENTITY_LABEL FROM  [dbo].[HFM_OUT_IS]&#xA;&#x9;) B&#xA;&#x9;ON A.LEGAL_ENTITY=B.ENTITY_LABEL&#xA;&#xA;&#xA;IF EXISTS (SELECT TOP 1 LEGAL_ENTITY FROM #TMP_COUNT WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'Legal Entity')&#xA;&#xA; BEGIN&#xA;&#xA;         DECLARE @LE_Count INT&#xA;         SET @LE_Count =&#xA;         (&#xA;         SELECT A.CNT-B.CNT AS Res_CNT    FROM&#xA;         (&#xA;         SELECT 'LE'AS NAME,COUNT(DISTINCT PARAMETER_VALUE)AS CNT FROM #TMP_COUNT  WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'Legal Entity' &#xA;         ) A&#xA;         INNER JOIN&#xA;         (&#xA;         SELECT 'LE'AS NAME,COUNT(DISTINCT PARAMETER_VALUE)AS CNT FROM #TMP_COUNT A WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'Legal Entity' &#xA;         AND ENTITY_LABEL IS  NULL&#xA;         AND NOT EXISTS &#xA;         (&#xA;         SELECT DISTINCT PARAMETER_VALUE FROM #TMP_COUNT B  WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'Legal Entity' &#xA;         AND ENTITY_LABEL IS NOT NULL&#xA;         AND A.PARAMETER_VALUE = B.PARAMETER_VALUE)&#xA;         )B&#xA;         ON A.NAME = B.NAME&#xA;         )&#xA;&#xA;       --More than 1 or  1 Legal entity missing scenario&#xA;       &#xA;       &#xA;       INSERT INTO [dbo].[HFM_PROCESS_STATUS_DETAIL]&#xA;       (&#xA;        HFM_PROCESS_STATUS_DETAIL_ID&#xA;       ,HFM_PROCESS_STATUS_REPORT_ID&#xA;       ,MESSAGE&#xA;       ,PROCESS_ITEM_STATUS_ID&#xA;       )&#xA;       &#xA;       SELECT&#xA;        NEWID()&#xA;       ,@HFM_PROCESS_STATUS_REPORT_ID&#xA;       ,'No data available in HFM system for the legal entity '+ PARAMETER_VALUE&#xA;       ,CASE WHEN @LE_Count = 0 THEN (select  PROCESS_STATUS_ID from [dbo].[PROCESS_STATUS_REF] where  PROCESS_STATUS_DESC IN  ('Errors'))&#xA;             WHEN @LE_Count &gt; 0 THEN (select  PROCESS_STATUS_ID from [dbo].[PROCESS_STATUS_REF] where  PROCESS_STATUS_DESC IN  ('Completed with Warnings'))&#xA;        END PROCESS_ITEM_STATUS_ID&#xA;       &#xA;       FROM #TMP_COUNT&#xA;       WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'Legal Entity'&#xA;       AND ENTITY_LABEL IS NULL&#xA;       &#xA;       --More than 1 or  1 Legal entity missing scenario&#xA;&#xA;&#xA; END&#xA;&#xA;IF EXISTS (SELECT TOP 1 LEGAL_ENTITY FROM #TMP_COUNT WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'MFRP Unit')&#xA;&#xA; BEGIN&#xA;&#xA;      DECLARE @MFRP_Count INT&#xA;      SET @MFRP_Count =&#xA;      (&#xA;      SELECT A.CNT-B.CNT AS Res_CNT    FROM&#xA;      (&#xA;      SELECT 'MFRP'AS NAME,COUNT(DISTINCT PARAMETER_VALUE)AS CNT FROM #TMP_COUNT  WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'MFRP Unit' &#xA;      ) A&#xA;      INNER JOIN&#xA;      (&#xA;      SELECT 'MFRP'AS NAME,COUNT(DISTINCT PARAMETER_VALUE)AS CNT FROM #TMP_COUNT A WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'MFRP Unit' &#xA;      AND ENTITY_LABEL IS  NULL&#xA;      AND NOT EXISTS &#xA;      (&#xA;      SELECT DISTINCT PARAMETER_VALUE FROM #TMP_COUNT B  WHERE HFM_PROCESS_PARAMETER_TYPE_DESC = 'MFRP Unit' &#xA;      AND ENTITY_LABEL IS NOT NULL&#xA;      AND A.PARAMETER_VALUE = B.PARAMETER_VALUE)&#xA;      )B&#xA;      ON A.NAME = B.NAME&#xA;      )&#xA;&#xA;&#x9;  --More than 1 or 1 MFRP Unit Legal entity missing scenario&#xA;&#xA;&#xA;    INSERT INTO [dbo].[HFM_PROCESS_STATUS_DETAIL]&#xA;    (&#xA;     HFM_PROCESS_STATUS_DETAIL_ID&#xA;    ,HFM_PROCESS_STATUS_REPORT_ID&#xA;    ,MESSAGE&#xA;    ,PROCESS_ITEM_STATUS_ID&#xA;    )&#xA;    &#xA;    SELECT&#xA;     NEWID()&#xA;    ,@HFM_PROCESS_STATUS_REPORT_ID&#xA;    ,'No data available in HFM system for the MFRP Unit'+ A.PARAMETER_VALUE&#xA;    ,CASE WHEN @MFRP_Count = 0 THEN (select  PROCESS_STATUS_ID from [dbo].[PROCESS_STATUS_REF] where  PROCESS_STATUS_DESC IN  ('Errors'))&#xA;          WHEN @MFRP_Count &gt; 0 THEN (select  PROCESS_STATUS_ID from [dbo].[PROCESS_STATUS_REF] where  PROCESS_STATUS_DESC IN  ('Completed with Warnings'))&#xA;     END PROCESS_ITEM_STATUS_ID&#xA;    &#xA;    FROM &#xA;    (&#xA;   