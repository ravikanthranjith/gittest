                      SQLTask:SqlStatementSource="&#xA;&#xA;DECLARE @LegalEntities varchar(4000), @Period varchar(20),@Year varchar(10),@DataVersionTypeDesc varchar(100),@HFM_PROCESS_STATUS_REPORT_ID UNIQUEIDENTIFIER&#xA;,@DATA_VERSION_ID bigint&#xA;&#xA;SET @LegalEntities = NULL&#xA;SET @HFM_PROCESS_STATUS_REPORT_ID = ?&#xA;&#xA;SELECT &#xA;@LegalEntities= COALESCE(@LegalEntities + ',','') + LEGAL_ENTITY&#xA;,@Period=P.pERIOD&#xA;,@Year=DV.YEAR&#xA;,@DataVersionTypeDesc=DVT.DATA_VERSION_TYPE_DESC&#xA;,@DATA_VERSION_ID = PS.DATA_VERSION_ID&#xA;&#xA;FROM DBO.HFM_Execution_parameter HEP&#xA;JOIN dbo.HFM_PROCESS_STATUS_REPORT PS ON HEP.HFM_PROCESS_STATUS_REPORT_ID=ps.HFM_PROCESS_STATUS_REPORT_ID&#xA;JOIN [dbo].[HFM_PROCESS_PARAMETER_TYPE_REF] HParamTypeRef&#x9;on HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_ID=HEP.HFM_PROCESS_PARAMETER_TYPE_ID&#xA;JOIN [dbo].[LEGAL_ENTITY_HIERARCHY] LE &#xA;ON  HEP.PARAMETER_VALUE= CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_DESC='Legal Entity' THEN LE.LEGAL_ENTITY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN HParamTypeRef.HFM_PROCESS_PARAMETER_TYPE_DESC='MFRP Unit' THEN LE.MFRP_UNIT&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE LE.MAN_COUNTRY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;JOIN DBO.PERIOD P ON  PS.PERIOD_ID=P.PERIOD_ID&#xA;JOIN DBO.data_version DV ON DV.DATA_VERSION_ID=PS.DATA_VERSION_ID&#xA;JOIN DBO.DATA_VERSION_TYPE DVT ON DV.DATA_VERSION_TYPE_ID=DVT.DATA_VERSION_TYPE_ID&#xA;WHERE HEP.HFM_PROCESS_STATUS_REPORT_ID= @HFM_PROCESS_STATUS_REPORT_ID&#xA;AND LE.VERSION_ID=1&#xA;&#xA;&#xA;SELECT  @LegalEntities As LegalEntities,@Period AS Period,@Year AS Year,@DataVersionTypeDesc as DataVersionTypeDesc,@DATA_VERSION_ID AS DATA_VERSION_ID"
